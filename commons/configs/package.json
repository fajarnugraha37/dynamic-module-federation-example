{
  "name": "@commons/configs",
  "version": "1.0.0",
  "description": "Shared Vite and TypeScript configurations for microfrontends",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "main": "index.ts",
  "exports": {
    "./postcss.config": "./postcss.config.js",
    "./vite.vue2": "./vite.vue2.js",
    "./vite.vue3": "./vite.vue3.js",
    "./vite.vue2.module-federation": "./vite.vue2.module-federation.js",
    "./vite.vue3.module-federation": "./vite.vue3.module-federation.js",
    "./vite.ssg": "./vite.ssg.config.js",
    "./vite.env": "./vite.env.config.js",
    "./tsconfig": "./tsconfig.json",
    "./tsconfig.vue2": "./tsconfig.vue2.json",
    "./tsconfig.vue3": "./tsconfig.vue3.json",
    "./tsconfig.mf": "./tsconfig.module-federation.json"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:vue2": "tsc --noEmit -p tsconfig.vue2.json",
    "type-check:vue3": "tsc --noEmit -p tsconfig.vue3.json",
    "type-check:mf": "tsc --noEmit -p tsconfig.module-federation.json"
  },
  "dependencies": {
  },
  "devDependencies": {
  },
  "peerDependencies": {
  },
  "optionalDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "vite-plugin-sitemap": "^0.7.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-robots-txt": "^1.0.0"
  },
  "keywords": [
    "vite",
    "vue",
    "microfrontends",
    "module-federation",
    "qiankun",
    "typescript",
    "config"
  ],
  "author": "Microfrontends Team",
  "license": "MIT"
}
